{
    "name": "nonetallt/laravel-resource-boiler",
    "description": "Create boilerplate code for laravel resources.",
    "type": "library",
    "license": "MIT",
    "require": {

    },
    "require-dev": {
        "orchestra/testbench": "^8.18",
        "pestphp/pest": "^2.30"
    },
    "autoload": {
        "psr-4": {
            "Nonetallt\\LaravelResourceBoiler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nonetallt\\LaravelResourceBoiler\\ServiceProvider"
            ]
        }
    }
}
