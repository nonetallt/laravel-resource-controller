{
    "name": "nonetallt/laravel-resource-boiler",
    "description": "Create boilerplate code for laravel resources.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nonetallt\\LaravelResourceBoiler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
